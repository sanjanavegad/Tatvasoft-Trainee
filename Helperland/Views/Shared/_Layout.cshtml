<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Helperland</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;700&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.ne`t/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <div class="wrapper" id="navbar-provider">
            <nav id="sidebar">
                <div class="container-fluid">
                    <div class="row row1">
                        <strong>Welcome, Sandeep.</strong>
                    </div>

                    <div class="row row2 ">
                        <ul class="nav nav-tabs flex-column">
                            <li class="nav-item">
                                <a class="nav-link" href="Service_History.html">Dashboard</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="Service_History.html">Service History</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="Service_History.html">Service Schedule</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="Service_History.html">Favorite Pros</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="Customer_Settings.html">My Settings</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="Home.html">Logout</a>
                            </li>

                        </ul>
                    </div>
                    <div class="row row3">
                        <ul class="navbar-nav">

                            <li class="nav-item">
                                <a class="nav-link login-trigger btn">Login</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="#" asp-action="#">Book a Cleaner</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="PublicPages" asp-action="Prices">Price & services</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="#" asp-action="#">Warranty</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="PublicPages" asp-action="Contact">Contact </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="#" asp-action="#">Become a Helper
                                </a>
                            </li>
                        </ul>

                    </div>
                    <hr>
                    <div class="row4">
                        <img class="socialmedia" src="../images/ic-facebook.png" alt="" style="margin-right: 40px; ">
                        <img class="socialmedia" src="../images/ic-instagram.png" alt="">
                    </div>
                </div>

            </nav>
            <div id="nevbar">
                <section class="small-nav">
                    <nav class="navbar navbar-expand-lg navbar-dark mynav" id="navbar"
                         style="position: fixed; width: 100%; z-index: 1;">
                        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="../images/logo-small.png" alt="Helperland"></a>
                        <button class="navbar-toggler" id="sidebarCollapse" type="button">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse justify-content-end" id="navbarNavDropdown">
                            <ul class="navbar-nav ms-auto mb-2 mb-lg-0 ">
                                <li class="nav-item">
                                    <a class="nav-link btn btn-primary text-white" style="padding-left: 15px;padding-right: 15px;"
                                       asp-area="" asp-controller="#" asp-action="#"> Book a Cleaner
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" asp-area="" asp-controller="PublicPages" asp-action="Prices">Prices</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="About.html">Our Guarantee</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="#service">Blog</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" asp-area="" asp-controller="PublicPages" asp-action="Contact">Contact Us</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link btn btn-primary text-white"
                                       style="padding-left: 20px;padding-right: 20px; margin-bottom: 4px;" href="#">Login</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link btn btn-primary text-white" style="padding-left: 15px;padding-right: 15px;"
                                       href="Service_Provider.html">
                                        Become a
                                        Helper
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </section>
            </div>

            <div class="overlay"></div>

        </div>
    </header>
    <div>
        @RenderBody()
    </div>
    <footer class="site-footer">
        <div class="footer-top">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-2 footer-widget">
                        <a asp-area="" asp-controller="Home" asp-action="Index" title="Helper Hand">
                            <img src="../images/footer-logo.png" alt="Helper Hand">
                        </a>
                    </div>
                    <div class="col-lg-8 footer-widget">
                        <ul class="footer-navigation d-flex justify-content-center">
                            <li>
                                <a asp-area="" asp-controller="Home" asp-action="Index" title="Home">Home</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="PublicPages" asp-action="About" title="About">About</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="#" asp-action="#" title="Testimonials">Testimonials</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="PublicPages" asp-action="FAQ" title="FAQs">FAQs</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="#" asp-action="#" title="Insurance Policy">Insurance Policy</a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="#" asp-action="#" title="Impressum">Impressum</a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-lg-2 footer-widget">
                        <ul class="social-media-list d-flex justify-content-end">
                            <li>
                                <a href="#" target="_blank" title="Facebook">
                                    <img src="../images/ic-facebook.png" alt="Facebook">
                                </a>
                            </li>
                            <li>
                                <a href="#" target="_blank" title="Instagram">
                                    <img src="../images/ic-instagram.png" alt="Instagram">
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <hr>
                <div class="justify-content-lg-center text-center" style="font-size: 16px;">
                    <div>
                        ©2018 Helperland. All rights reserved. &nbsp;&nbsp;&nbsp;&nbsp; <a href=""
                                                                                           class="row-bottom">
                            Terms
                            and Conditions
                        </a> | <a href="" class="row-bottom">Privacy Policy</a>
                    </div>
                </div>
            </div>
        </div>
    </footer>


    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
            integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
            crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
            integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
            crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
            integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
            crossorigin="anonymous"></script>
    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
            crossorigin="anonymous"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
